{
    "name": "asgrim/example-pie-extension",
    "type": "php-ext",
    "license": "MIT",
    "description": "Example PIE extension",
    "require": {
        "php": ">=8.3,<8.4"
    },
    "replace": {
        "ext-example_pie_extension": "*"
    },
    "php-ext": {
        "extension-name": "ext-example_pie_extension",
        "priority": 74,
        "configure-options": [
            {
                "name": "enable-example-pie-extension",
                "description": "whether to enable example-pie-extension support"
            }
        ]
    }
}
